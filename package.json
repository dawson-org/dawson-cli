{
  "name": "dawson",
  "version": "0.9.0",
  "description": "Serverless Web Framework for AWS Lambda and API Gateway",
  "repository": "https://github.com/lusentis/dawson",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "compile": "babel -s true --source-maps inline -d dist/ src/ --ignore *.AppleDouble*",
    "coverage": "tap --nyc-arg=--require --nyc-arg=babel-register src/**/*.spec.js --cov --coverage-report=html",
    "link:watch": "nodemon -w src -x npm -- run lint-link",
    "lint": "semistandard --fix | snazzy",
    "lint-link": "npm run lint && npm link",
    "lint:watch": "nodemon -w src -x npm -- run lint",
    "prepublish": "npm run clean && npm run compile",
    "test": "npm run lint && tap --nyc-arg=--require --nyc-arg=babel-register src/**/*.spec.js --check-coverage --lines 0"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Simone Lusenti <lusenti.s@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "aws-sdk": "^2.7.17",
    "babel-polyfill": "^6.16.0",
    "babel-register": "^6.16.3",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "common-tags": "^1.3.1",
    "console.table": "^0.8.0",
    "del": "^2.2.2",
    "docker-lambda": "^0.9.1",
    "es6-promisify": "^5.0.0",
    "execa": "^0.6.0",
    "hard-rejection": "^0.1.0",
    "http-proxy": "^1.14.0",
    "indent-string": "^3.0.0",
    "json5": "^0.5.1",
    "listr": "^0.9.0",
    "listr-verbose-renderer": "^0.2.0",
    "lodash": "^4.17.2",
    "minimatch": "^3.0.3",
    "moment": "^2.17.0",
    "node-watch": "^0.4.1",
    "pathmatch": "^0.0.2",
    "prettyjson": "^1.1.3",
    "prop-types": "^0.2.0",
    "send": "^0.14.1",
    "source-map-support": "^0.4.6",
    "tap": "^9.0.3",
    "temp": "^0.8.3",
    "update-notifier": "^1.0.2",
    "yargs": "^6.0.0",
    "zen-observable": "^0.4.0"
  },
  "bin": "dist/index.js",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.1.4",
    "pre-commit": "^1.2.2",
    "semistandard": "^9.0.0",
    "snazzy": "^5.0.0"
  }
}
