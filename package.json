{
  "name": "dawson",
  "version": "0.16.1",
  "description": "Serverless Web Framework for AWS Lambda and API Gateway",
  "repository": "https://github.com/lusentis/dawson",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "compile": "babel -s true --source-maps inline -d dist/ src/ --ignore *.AppleDouble*",
    "link:watch": "nodemon -w src -x npm -- run lint-link",
    "format": "prettier-eslint --write src/**/*.js | snazzy",
    "lint": "npm run format",
    "lint-link": "npm run lint && npm link",
    "lint:watch": "nodemon -w src -x npm -- run lint",
    "prepublish": "npm run clean && npm run compile",
    "pre-commit": "npm run lint",
    "coverage-report": "nyc report --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter=lcov",
    "test": "nyc ava src/**/*.spec.js && npm run coverage-report"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Simone Lusenti <lusenti.s@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "aws-sdk": "^2.7.17",
    "babel-polyfill": "^6.16.0",
    "babel-register": "^6.16.3",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "common-tags": "^1.3.1",
    "console.table": "^0.8.0",
    "del": "^2.2.2",
    "docker-lambda": "^0.11.0",
    "es6-promisify": "^5.0.0",
    "execa": "^0.6.0",
    "hard-rejection": "^1.0.0",
    "http-proxy": "^1.14.0",
    "indent-string": "^3.0.0",
    "json5": "^0.5.1",
    "listr": "^0.10.0",
    "listr-verbose-renderer": "^0.3.0",
    "lodash": "^4.17.2",
    "minimatch": "^3.0.3",
    "moment": "^2.17.0",
    "pathmatch": "^0.0.2",
    "prettyjson": "^1.1.3",
    "prop-types": "^0.2.0",
    "send": "^0.14.1",
    "source-map-support": "^0.4.6",
    "temp": "^0.8.3",
    "update-notifier": "^1.0.2",
    "yargs": "^6.0.0",
    "zen-observable": "^0.4.0"
  },
  "bin": "dist/index.js",
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.1.4",
    "deep-sort-object": "^1.0.1",
    "eslint": "^3.14.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.2",
    "prettier-eslint-cli": "^1.1.0",
    "snazzy": "^6.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": true
  }
}
