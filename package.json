{
  "name": "dawson",
  "version": "1.0.0-beta.10",
  "description": "Serverless Web Framework for AWS Lambda and API Gateway",
  "repository": "https://github.com/lusentis/dawson",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -s true -d lib/ src/",
    "compile:watch": "babel --watch -s true -d lib/ src/",
    "dev": "npm run compile:watch",
    "clean": "rm -rf lib",
    "prepublish": "npm run clean && npm run compile",
    "test": "npm run compile && npm run lint && npm run tape",
    "lint": "semistandard | snazzy",
    "tape": "NODE_ENV=testing tape -r babel-register src/**/*.spec.js | tap-dot"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "author": "Simone Lusenti <lusenti.s@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "aws-sdk": "^2.6.3",
    "babel-runtime": "^6.11.6",
    "colors": "^1.1.2",
    "common-tags": "^1.3.1",
    "console.table": "^0.7.0",
    "es6-promisify": "^4.1.0",
    "expect.js": "^0.3.1",
    "http-proxy": "^1.14.0",
    "indent-string": "^3.0.0",
    "mime": "^1.3.4",
    "moment": "^2.14.1",
    "prettyjson": "^1.1.3",
    "progress": "^1.1.8",
    "recursive-readdir": "^2.0.0",
    "send": "^0.14.1",
    "simple-spinner": "0.0.5",
    "temp": "^0.8.3",
    "yargs": "^5.0.0"
  },
  "bin": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "pre-commit": "^1.1.3",
    "semistandard": "^9.0.0",
    "snazzy": "^5.0.0",
    "tap-dot": "^1.0.5",
    "tape": "^4.6.0"
  }
}
